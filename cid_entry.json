{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "definitions": {
	  "movement_order": {
		  "type": "object",
		  "properties": {
			"Location": {
			  "description": "Name of centre",
			  "type": "string",
			  "pattern": "^[a-z]{3,30}$"
			},
			"MO In/MO Out": {
				"description": "Direction of movement order",
				"type": "string",
				"enum": [
					"In",
					"Out"
				]
			},
			"MO Ref": {
				"desceiption": "Movement order reference",
				"type": "integer",
				"maxLength": 100,
				"minimum": 0
			},
			"CID Person ID": {
				"description": "Case information database identifer",
				"type": "integer",
				"maxLength": 100,
				"minimum": 0
			},
			"MO Date": {
				"description": "Date of movement order",
				"type": "string",
				"format": "date-time"
			},
			"MO Type": {
				"description": "Type of movement order",
				"type": "string",
				"enum": [
					"Occupancy",
					"Non-Occupancy",
					"Removal"
				]
			}
		  },
		  "required": [
			  "Location",
			  "MO In/MO Out",
			  "MO Ref",
			  "CID Person ID",
			  "MO Date",
			  "MO Type"
		  ]
	  }
  },
  "properties": {
	  "cDataSet": {
		  "type": "array",
		  "$ref": "#/definitions/movement_order"
	  }
  }
}
