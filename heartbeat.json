{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "centre": {
      "description": "Name of centre",
      "type": "string",
      "pattern": "^[a-z]{3,30}$"
    },
    "male": {
      "description": "Number of male beds occupied",
      "type": "integer",
      "minimum": 0,
      "maximum": 5000
    },
    "female": {
      "description": "Number of female beds occupied",
      "type": "integer",
      "minimum": 0,
      "maximum": 5000
    },
    "ooc_male": {
      "description": "Number of male out of commission beds",
      "type": "integer",
      "minimum": 0,
      "maximum": 5000
    },
    "ooc_female": {
      "description": "Number of female out of commission beds",
      "type": "integer",
      "minimum": 0,
      "maximum": 5000
    }
  },
  "required": [
    "centre",
    "male",
    "female",
    "ooc_male",
    "ooc_female"
  ]
}
